@{
}
<form>
    <label class="form-control" asp-for="Model">Model samochodu: @Model.Model</label>
    <label class="form-control" asp-for="Producent">Producent samochodu: @Model.Producent</label>
    <label class="form-control" asp-for="PojemmoscSilnika">Pojemność silnika: @Model.PojemmoscSilnika</label>
    <label class="form-control" asp-for="Moc">Moc samochodu: @Model.Moc</label>
    <label class="form-control" asp-for="RodzajSilnika">Rodzaj silnika: @Model.RodzajSilnika</label>
    <label class="form-control" asp-for="NrRejestracyjny">Nr. rejestracyjny samochodu: @Model.NrRejestracyjny</label>
    <label class="form-control" asp-for="Wlasciciel">Właściciel samochodu: @Model.Wlasciciel</label>
    <button asp-action="Index" asp-controller="Car">Wróć</button>
</form>